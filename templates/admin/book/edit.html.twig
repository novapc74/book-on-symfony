{% extends 'admin/layout/layout_main.html.twig' %}

{% block body %}
<div class="container py-4">
    <h4>Create / Edit Book</h4>
    <div class="row">
        <div class="coll-md-9">
            {{form_start(form)}}
                {{ form_row(form.name) }}
                {{ form_row(form.subName) }}
                <input class="form-control W-50" type='submit' values='Create'>
            {{form_end(form)}}
        </div>
    </div>
</div>
{% endblock %}